{
    "name": "Live2dRender",
    "private": true,
    "scripts": {
        "start": "vite --mode development",
        "dev": "vite --mode development",
        "build": "vite build --mode production",
        "build:web": "cross-env BUILD_TARGET=web vite build --mode production",
        "build:node": "cross-env BUILD_TARGET=node vite build --mode production",
        "test": "tsc --noEmit",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "serve": "vite preview",
        "clean": "rimraf dist"
    },
    "module": "./dist/live2d-render.es.js",
    "main": "./dist/live2d-render.cjs",
    "exports": {
        ".": {
            "import": "./dist/live2d-render.es.js",
            "require": "./dist/live2d-render.cjs"
        }
    },
    "types": "./dist/live2d-render.d.ts",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "serve": "^14.2.0",
        "typescript": "^5.0.4",
        "vite": "npm:rolldown-vite@^7.2.5",
        "tslib": "^2.6.2"
    },
    "dependencies": {
        "live2d-render": "^0.0.1",
        "whatwg-fetch": "^3.6.2"
    },
    "optionalDependencies": {
        "fsevents": "*"
    }
}
